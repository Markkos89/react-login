        "fastify-cors": "^1.0.1",
        "fastify-mongodb": "^0.9.1",
        "fastify-swagger": "^0.15.3",
        "mongoose": "^5.3.14",
        "nodemon": "^1.18.7",
        "pm2": "^3.3.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.5",
